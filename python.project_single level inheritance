      "cell_type": "code",
      "source": [
        "# Parent Class for 12th Grade Resume\n",
        "class Resume12th:\n",
        "    resume_type = \"12th Grade Resume\"\n",
        "\n",
        "    def __init__(self, name, email, phone, year_of_passout, marks, school_name, board):\n",
        "        self.name = name\n",
        "        self.email = email\n",
        "        self.phone = phone\n",
        "        self.year_of_passout = year_of_passout\n",
        "        self.marks = marks\n",
        "        self.school_name = school_name\n",
        "        self.board = board\n",
        "\n",
        "    def display(self):\n",
        "        print(\"\\n--- 12th Grade Resume ---\")\n",
        "        print(f\"Resume Type: {Resume12th.resume_type}\")\n",
        "        print(f\"Name: {self.name}\")\n",
        "        print(f\"Email: {self.email}\")\n",
        "        print(f\"Phone: {self.phone}\")\n",
        "        print(f\"Year of Passout: {self.year_of_passout}\")\n",
        "        print(f\"Marks: {self.marks}%\")\n",
        "        print(f\"School: {self.school_name}\")\n",
        "        print(f\"Board: {self.board}\")\n",
        "        return self  # Method Chaining\n",
        "\n",
        "\n",
        "# Child Class for Degree-Level Resume\n",
        "class DegreeResume(Resume12th):\n",
        "    def __init__(self, name, email, phone, year_of_passout, marks, school_name, board,\n",
        "                 degree_name, university, cgpa, specialization, internship, projects):\n",
        "        super().__init__(name, email, phone, year_of_passout, marks, school_name, board)\n",
        "        self.degree_name = degree_name\n",
        "        self.university = university\n",
        "        self.cgpa = cgpa\n",
        "        self.specialization = specialization\n",
        "        self.internship = internship\n",
        "        self.projects = projects\n",
        "\n",
        "    def display(self):\n",
        "        super().display()  # Show parent class details\n",
        "        print(\"\\n--- Degree Resume Details ---\")\n",
        "        print(f\"Degree: {self.degree_name}\")\n",
        "        print(f\"University: {self.university}\")\n",
        "        print(f\"CGPA: {self.cgpa}\")\n",
        "        print(f\"Specialization: {self.specialization}\")\n",
        "        print(f\"Internship: {self.internship}\")\n",
        "        print(f\"Projects: {self.projects}\")\n",
        "        return self  # Method Chaining\n",
        "\n",
        "\n",
        "# Example\n",
        "student = DegreeResume(\n",
        "    name=\"Sruthi\",\n",
        "    email=\"srthi@example.com\",\n",
        "    phone=\"9876543210\",\n",
        "    year_of_passout=2020,\n",
        "    marks=80,\n",
        "    school_name=\"National Model\",\n",
        "    board=\"CBSE\",\n",
        "    degree_name=\"B.Sc\",\n",
        "    university=\"Bharathi University\",\n",
        "    cgpa=8.6,\n",
        "    specialization=\"Mathematics\",\n",
        "    internship=\"Data Analytics at spill Ltd.\",\n",
        "    projects=\"Data Analysis Tool, Resume Builder App\"\n",
        ")\n",
        "\n",
        "# Method Chaining in action\n",
        "student.display()\n"
